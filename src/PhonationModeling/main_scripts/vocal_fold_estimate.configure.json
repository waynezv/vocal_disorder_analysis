{
    "__comment__": "Experiment",
    "data_root": "data/creaky_voice",
    "wav_dir": "processed/phone_AA1_creaky",
    "glottal_flow_dir": "glottal_flows/phone_AA1_creaky",
    "list_dir": "src/PhonationModeling/data/filelists",
    "wav_list": "phone_segs_AA1_creaky.lst",
    "glottal_flow_list": "phone_segs_AA1_creaky_glottal_flow.lst",
    "optim_patience": 400,
    "results_save_dir": "src/PhonationModeling/main_scripts/outputs/vocal_fold_estimate/results",
    "results_save_filename": "best_results_08242020_AA1",
    "__comment__": "Log",
    "verbose": false,
    "log_dir": "src/PhonationModeling/main_scripts/outputs/vocal_fold_estimate/logs",
    "log": {
        "version": 1,
        "disable_existing_loggers": false,
        "formatters": {
            "custom": {
                "format": "%(asctime)s - [%(name)s - %(funcName)-8.8s:%(lineno)-4d] - [%(levelname)-5.5s] - %(message)s",
                "datefmt": "%Y/%m/%d %H:%M"
            }
        },
        "handlers": {
            "console": {
                "class": "logging.StreamHandler",
                "level": "DEBUG",
                "formatter": "custom",
                "stream": "ext://sys.stdout"
            },
            "file": {
                "class": "logging.handlers.RotatingFileHandler",
                "level": "DEBUG",
                "formatter": "custom",
                "filename": "src/PhonationModeling/main_scripts/outputs/vocal_fold_estimate/logs/run_08242020_AA1",
                "maxBytes": 10485760,
                "backupCount": 20,
                "encoding": "utf8"
            }
        },
        "loggers": {
            "main": {
                "level": "DEBUG",
                "handlers": [
                    "console",
                    "file"
                ],
                "propagate": false
            }
        },
        "root": {
            "level": "DEBUG",
            "handlers": []
        }
    }
}