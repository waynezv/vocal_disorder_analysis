{
    "__comment__": "Experiment",
    "data_root": "creaky_voice/data",
    "wav_dir": "processed/phone_L_normal",
    "glottal_flow_dir": "glottal_flows/phone_L_normal",
    "wav_list": "creaky_voice/src/data/phone_segs_L_normal.lst",
    "glottal_flow_list": "creaky_voice/src/data/phone_segs_L_normal_glottal_flow.lst",
    "optim_patience": 200,
    "results_save_dir": "creaky_voice/src/main_scripts/outputs/vocal_fold_estimate/results",
    "results_save_filename": "best_results_08222020",
    "__comment__": "Log",
    "verbose": false,
    "log_dir": "creaky_voice/src/main_scripts/outputs/vocal_fold_estimate/logs",
    "log": {
        "version": 1,
        "disable_existing_loggers": false,
        "formatters": {
            "custom": {
                "format": "%(asctime)s - [%(name)s - %(funcName)-8.8s:%(lineno)-4d] - [%(levelname)-5.5s] - %(message)s",
                "datefmt": "%Y/%m/%d %H:%M"
            }
        },
        "handlers": {
            "console": {
                "class": "logging.StreamHandler",
                "level": "DEBUG",
                "formatter": "custom",
                "stream": "ext://sys.stdout"
            },
            "file": {
                "class": "logging.handlers.RotatingFileHandler",
                "level": "DEBUG",
                "formatter": "custom",
                "filename": "creaky_voice/src/main_scripts/outputs/vocal_fold_estimate/logs/run_08222020",
                "maxBytes": 10485760,
                "backupCount": 20,
                "encoding": "utf8"
            }
        },
        "loggers": {
            "main": {
                "level": "DEBUG",
                "handlers": [
                    "console",
                    "file"
                ],
                "propagate": false
            }
        },
        "root": {
            "level": "DEBUG",
            "handlers": []
        }
    }
}